{
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "none",
    "printWidth": 100,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "useTabs": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css"
}